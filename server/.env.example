# Server
PORT=3000

# CORS (optional, comma-separated)
CORS_ORIGINS=http://localhost:19006,http://localhost:8081

# Dummy mode: enables stricter card validation, random tokenization, and dev seed endpoint
DUMMY_MODE=true

# Appwrite
APPWRITE_ENDPOINT=https://cloud.appwrite.io/v1
APPWRITE_PROJECT_ID=your_project_id
# Optional API key for server operations (create/list/delete documents)
APPWRITE_API_KEY=your_api_key
APPWRITE_DATABASE_ID=your_database_id
APPWRITE_CARDS_COLLECTION_ID=your_cards_collection_id
APPWRITE_TRANSACTIONS_COLLECTION_ID=your_transactions_collection_id
APPWRITE_NOTIFICATIONS_COLLECTION_ID=your_notifications_collection_id
