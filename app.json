{
  "expo": {
    "androidNavigationBar": {
      "backgroundColor": "#ffffff",
      "barStyle": "dark-content"
    },
    "androidStatusBar": {
      "backgroundColor": "#0F766E",
      "barStyle": "light-content"
    },
    "name": "Ahantaman Rural Bank",
    "slug": "ahantaman-rural-bank",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "ahantamanbank",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0F766E"
    },

    "extra": {
      "firebaseConfig": {
        "apiKey": "EXPO_PUBLIC_FIREBASE_API_KEY",
        "authDomain": "EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN",
        "projectId": "EXPO_PUBLIC_FIREBASE_PROJECT_ID",
        "databaseURL": "EXPO_PUBLIC_FIREBASE_DATABASE_URL",
        "messagingSenderId": "EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID",
        "appId": "EXPO_PUBLIC_FIREBASE_APP_ID"
      },
      "router": {},
      "eas": {
        "projectId": "a797e361-181e-421f-8e99-febdf932bfa1"
      }
    },

    "ios": {
      "bundleIdentifier": "com.ahantamanruralbank.app",
      "supportsTablet": true,
      "infoPlist": {
        "NSFaceIDUsageDescription": "Ahantaman Rural Bank uses Face ID for secure and convenient authentication."
      }
    },
    "android": {
      "package": "com.ahantamanruralbank.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "USE_BIOMETRIC",
        "USE_FINGERPRINT",
        "USE_BIOMETRIC",
        "USE_FINGERPRINT"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#0F766E",
          "image": "./assets/images/splash-icon.png",
          "resizeMode": "contain",
          "hideAsync": false
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "90c0a4d9-ed38-47b5-bdee-97664c4efe9f"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/90c0a4d9-ed38-47b5-bdee-97664c4efe9f"
    }
  }
}
